<div class="event-occations row">
  {% for year, event_occations_by_month in event_occations_by_year_month %}
    {% if (loop.index - 1) %}
      <div class="col-lg-6 col-lg-offset-3 col-sm-8 col-sm-offset-2">
          <h2 class="event-year">{{ year }}</h2>
      </div>
    {% endif %}
    {% for month, event_occations in event_occations_by_month %}
    <div class="col-sm-10 col-md-8 col-lg-6  col-lg-offset-3 col-sm-offset-1 col-md-offset-2">
      <div>
        <h2 class="event-month">{{ month }}</h2>
      </div>
      <div class="event-occations-by-month">
        {{ event_occations | raw }}
      </div>
      <div class="divider"></div>
    </div>
    {% endfor %}
  {% endfor %}
