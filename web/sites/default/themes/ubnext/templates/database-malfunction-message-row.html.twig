{# slate
  name = Database message: item
  models[database] = entity:node
#}
<div class="row">
	<div class="col-xs-12">
	  <div class="alert alert-warning" role="alert">
	    <div class="alert-icon"><i class="fa fa-info-circle"></i></div>
	    <div class="alert-body">
	      <div class="alert-database-title"><strong>{{database.title.value}}</strong></div>
	      <div class="alert-error-message">{{database.field_malfunction_message.value | raw}}</div>
	    </div>
	  </div>
	</div>
</div>
