{% import "macros/dates.html.twig" as dates %}
{% import "macros/misc.html.twig" as misc %}
<div class="story-teaser">
  <div class="story-teaser-publication-date date">{{ dates.date_day_and_month(story.created) }}</div>
  <div class="story-teaser-header">{{ misc.link(story.title_field.value, story.url) }}</div>
</div>
